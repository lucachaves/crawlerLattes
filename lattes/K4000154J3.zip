<script>

		function updateCaptcha() {
        	var src = "/buscatextual/sevletcaptcha?&ts=" + (new Date()).getTime();
        	document.getElementById("imgCaptcha").src = src;
        	document.getElementById('palavra').focus();
        }
        function alertaMensagem(msg){	
           alert(msg);
           updateCaptcha();
        }
        function fecharModalCaptcha(){
            document.getElementById('captcha').style.display = 'none';	
            document.getElementById('botaoEnviar').style.display = 'inline';
            document.getElementById('botaoCancelar').style.display = 'inline';
        }
</script>


<style>
    .captcha {
		border: 2px solid #000000;
    	border-radius: 5px 5px 5px 5px;
    	margin: 116px auto 0;
    	min-height: 176px;
    	padding: 0 1px;
    	position: static;
    	width: 310px;
    	z-index: 9999;
	}
</style>


   	
   	
